{
    "name": "storefront-backend",
    "version": "1.0.0",
    "description": "A store-api, to handle company products",
    "main": "index.ts",
    "scripts": {
        "start": "npx nodemon src/index.ts",
        "clear": "rimraf build/ || true",
        "build": "npm run clear && npx tsc",
        "lint": "eslint . --ext .ts",
        "prettier": "prettier src/**/*.ts --write",
        "prod": "npm run build && nodemon build/index.js",
        "schema": "npx db-migrate up",
        "test": "set ENV=test&& db-migrate --env test up && npm run build && npx jasmine"
    },
    "author": "Mohammad-Hossam",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.2",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/node": "^16.11.12",
        "@types/pg": "^8.6.3",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "eslint": "^8.4.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^3.10.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "supertest": "^6.1.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.7.1"
    }
}
